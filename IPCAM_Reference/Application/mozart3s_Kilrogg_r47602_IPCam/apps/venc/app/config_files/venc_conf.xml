<?xml version="1.0" standalone="yes"?>
<root>
	<template>indoor</template>
	<sensor_config_path>#sensor_conf#</sensor_config_path>
	<sys_path>/sys/VideoSensorInfo</sys_path>
	<buffer_info>
		<vic_output>
		      <dram_location>#vic_output_dram_num#</dram_location>
		</vic_output>
		<ldc_ref_frame>
			<dram_location>#ldc_ref_frame_dram_num#</dram_location>
		</ldc_ref_frame>
		<dmac_output>
			<dram_location>#dmac_output_dram_num#</dram_location>
		</dmac_output>
		<ire_output0>
			<dram_location>#ire_output0_dram_num#</dram_location>
		</ire_output0>
		<ire_output1>
			<dram_location>#ire_output1_dram_num#</dram_location>
		</ire_output1>
		<bitstream_output>
			<dram_location>#bitstream_output_dram_num#</dram_location>
		</bitstream_output>
		<voc_output>
			<dram_location>#voc_output_dram_num#</dram_location>
		</voc_output>
		<motion_snapshot_output>
			<dram_location>#motion_snapshot_output_dram_num#</dram_location>
		</motion_snapshot_output>
	</buffer_info>
	<bus_info>
		<mp4v_bus_num>#mp4v_bus_num#</mp4v_bus_num>
		<h264_bus_num>#h264_bus_num#</h264_bus_num>
		<svc_bus_num>#svc_bus_num#</svc_bus_num>
	</bus_info>
	<venc_master>
		<initial>on</initial>
		<config_path>#master_conf#</config_path>
		<switch>0</switch>
    </venc_master>
    <venc_videoin>
    	<sharebuffer>
        	<path>/dev/buff_mgr##</path>
        </sharebuffer>
		<initial>on</initial>
    	<config_path>#videoin_conf#</config_path>
		<switch>0</switch>
    </venc_videoin>
    <venc_encoder>##</venc_encoder>
    <venc_motion>
		<initial>#venc_motion_initial#</initial>
    	<config_path>#vencmotion_conf#</config_path>
		<switch>0</switch>
    </venc_motion>
    <venc_display>
		<initial>#venc_display#</initial>
    	<config_path>#vencdisplay_conf#</config_path>
		<switch>0</switch>
    </venc_display>
</root>

